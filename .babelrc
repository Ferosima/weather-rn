{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx",
          ".android.js",
          ".android.tsx",
          ".ios.js",
          ".ios.tsx"
        ],
        "alias": {
          "@assets/*": ["./assets/*"],
          "@components/*": ["./src/components/*"],
          "@src/*": ["./src/*"]
        }
      }
    ]
  ]
}
